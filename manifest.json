{
    "name": "Tabs Export",
    "version": "0.1",
    "description": "Export URLs of all currently opened tabs",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistence": false
    },
    "browser_action": {
        "default_title": "Tabs Export",
        "default_icon": "images/tabs_export32.png",
        "default_popup": "popup.html"
    },
    "permissions": ["declarativeContent", "tabs", "notifications"],
    "icons": {
        "16": "images/tabs_export16.png",
        "32": "images/tabs_export32.png",
        "64": "images/tabs_export64.png",
        "128": "images/tabs_export128.png"
    }
}